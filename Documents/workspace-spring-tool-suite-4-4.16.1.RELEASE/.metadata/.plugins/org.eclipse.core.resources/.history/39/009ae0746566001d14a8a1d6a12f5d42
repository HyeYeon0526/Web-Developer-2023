package helloJavaProgramming;

import java.io.*;

public class main {


	public static void main(String[] args) throws IOException {
				
		PrintStream out = new PrintStream(new BufferedOutputStream(new FileOutputStream(new File(".../Desktop/hello/myfile.txt")),512));
		
		
		out.write("Hello java!!".getBytes());
		out.close();

	}

}
